{
  "version": 2,
  "buildCommand": "cd app && pnpm install && pnpm run build",
  "outputDirectory": "app/.next",
  "installCommand": "cd app && pnpm install",
  "framework": "nextjs",
  "functions": {
    "app/app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
